<div *ngFor="let item of items">
    <h1 mat-dialog-title>{{ headers }}</h1>
    
    <div class="row">
        <div class="col-12">
            <mat-form-field hintLabel="Max 3 characters">
                <mat-label>Code: </mat-label>
                <input matInput [(ngModel)]="item.gb_cell_code" #gb_cell_code required maxlength="3">
                <mat-hint align="end">{{item.gb_cell_code?.length || 0}}/3</mat-hint>
            </mat-form-field>
        </div>
    </div>
<br>
    <div class="row">
        <div class="col-12">
            <mat-form-field hintLabel="Max 20 characters">
                <mat-label>Detail: </mat-label>
                <input matInput [(ngModel)]="item.gb_cell_detail" #gb_cell_detail required maxlength="20">
                <mat-hint align="end">{{item.gb_cell_detail?.length || 0}}/20</mat-hint>
            </mat-form-field>
        </div>
    </div>

    <div class="row">
        <div class="col-2 offset-8"><button mat-button mat-dialog-close>No Thanks</button></div>
        <div class="col-2">
            <button mat-raised-button class="bg-0f4289" mat-dialog-close
                (click)="submit(button,item)">{{ button }}</button>
        </div>
    </div>
</div>