                            {{model.model_detail}}
                        </mat-option>
                    </mat-select>
                    <mat-error *ngIf="modelControl.hasError('required')">Please choose an model</mat-error>
                </mat-form-field>
            </div>
        </div>
        <div class="row pt-3">
