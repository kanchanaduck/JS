<div *ngFor="let item of items">
    <h1 mat-dialog-title>{{ headers }}</h1>
    <!-- style="visibility: hidden;" -->
    <!-- <input matInput [(ngModel)]="item.cell_code" #cell_code > -->
    <div class="row">
        <div class="col-12">
            <mat-form-field>
                <mat-label>Code: </mat-label>
                <input matInput [(ngModel)]="item.cell_code" #cell_code >
            </mat-form-field>
        </div>
    </div>

    <div class="row">
        <div class="col-12">
            <mat-form-field>
                <mat-label>Detail: </mat-label>
                <input matInput [(ngModel)]="item.cell_detail" #cell_detail required>
            </mat-form-field>
        </div>
    </div>

    <div class="row">
        <div class="col-2 offset-8"><button mat-button mat-dialog-close>No Thanks</button></div>
        <div class="col-2">
            <button mat-raised-button class="bg-0f4289" mat-dialog-close
                (click)="submit(button,item)">{{ button }}</button>
        </div>
    </div>
</div>