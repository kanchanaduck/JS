<div *ngFor="let item of items">
    <hr>
    <div class="row pt-3 f-10 text-right">
        <div class="col-5"></div>
        <div class="col-7">
            <button mat-raised-button class="bg-545E75 text-white" style="cursor: unset;">
                <span><label for="staticEmail" class="col-8 col-form-label">Total Overtime / Emergency :</label>
                </span>
                <input type="text" disabled="disabled" [value]="v_total_overtime" 
                [(ngModel)]="item.total_overtime" #txttotal_overtime
                    class="form-control col-4">
            </button>
        </div>
    </div>
